DATATYPE GRP1;
SORT
	GRP1;
CONST
	1: GRP1;
VAR
	A, B, C: GRP1;
OPERATOR
	*: GRP1, GRP1 -> GRP1;
	I: GRP1 -> GRP1;
NOTATION
	*, I: FUNCTION;
AXIOM
 [1] *(1,A)==A;
 [2] *(I(A),A)==1;
 [3] *(*(A,B),C)==*(A,*(B,C));
 [4] *(I(A),*(A,C))==C;
 [5] *(A,1)==A;
 [6] *(A,I(A))==1;
 [7] I(1)==1;
 [8] I(I(A))==A;
 [9] *(A,*(I(A),C))==C;
[10] I(*(A,B))==*(I(B),I(A));
END
