DATATYPE X8;
SORT
	X8;
CONST
	0, a, b, c: X8;
VAR
	A, B, C: X8;
OPERATOR
	+: X8, X8 -> X8;
	I: X8 -> X8;
NOTATION
	+, I: FUNCTION;
AXIOM
[1] +(0,A)==A;
[2] +(I(A),A)==0;
[3] +(+(A,B),C)==+(A,+(B,C));
[4] +(a,+(a,a))==0;
[5] +(b,b)==0;
[6] +(b,+(a,a))==+(a,b);
[7] +(c,c)==0;
[8] +(c,a)==+(a,c);
[9] +(c,b)==+(b,c);
END
