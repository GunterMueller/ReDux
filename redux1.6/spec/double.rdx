DATATYPE PEANOL;
SORT
	PEANOL;
CONST
	0: PEANOL;
VAR
	A, B, C, D, E: PEANOL;
OPERATOR
	*: PEANOL, PEANOL -> PEANOL;
	+: PEANOL, PEANOL -> PEANOL;
	S: PEANOL -> PEANOL;
	DOUBLE: PEANOL -> PEANOL;
NOTATION
	*, +, S, DOUBLE: FUNCTION;
AXIOM
[1] +(0,A)==A;
[2] +(S(B),A)==S(+(B,A));
[3] *(0,A)==0;
[4] *(S(B),A)==+(*(B,A),A);
[5] DOUBLE(0)==0;
[6] DOUBLE(S(A))==S(S(DOUBLE(A)));
END
