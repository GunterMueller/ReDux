DATATYPE INT;
SORT
	INT;
CONST
	0: INT;
VAR
	A, B, C: INT;
OPERATOR
	-: INT -> INT;
	s: INT -> INT;
	+: INT, INT -> INT;
NOTATION
        s: FUNCTION;
AXIOM
 [1] -0        == 0;
 [2] --A       == A;
 [3] s(-s(A))  == -A;
 [4] 0 + A     == A;
 [5] s(A) + B  == s(A + B);
 [6] -s(A) + B == -s(A + -B);
END
