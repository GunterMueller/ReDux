DATATYPE Z4;
SORT
	KOEF;
CONST
	0, 1: KOEF;
VAR
	a, b, c, d, e: KOEF;
OPERATOR
	+: KOEF, KOEF -> KOEF;
	*: KOEF, KOEF -> KOEF;
	~: KOEF -> KOEF;
NOTATION
	+, *, ~: FUNCTION;
THEORY
	+, *: AC;
AXIOM
[1] +(0,a) == a;
[2] *(0,a) == 0;
[3] *(1,a) == a;
[4] ~(0) == 0;
[5] ~(~(a)) == a;
[6] +(a,~(a)) == 0;
[7] ~(+(a,b)) == +(~(a),~(b));
[8] *(a,~(b)) == ~(*(a,b));
[9] *(a,+(b,c)) == +(*(a,b),*(a,c));

[10] ~(1) == +(1,+(1,1));

END
