DATATYPE LIST;
SORT
	EL, LIST;
CONST
	0: EL;
	NIL: LIST;
VAR
	A, B, C: EL;
	L, M, N: LIST;
OPERATOR
	APP: LIST, LIST -> LIST;
	CONS: EL, LIST -> LIST;
	REV: LIST -> LIST;
	REV2: LIST, LIST -> LIST;
NOTATION
	APP, CONS, REV, REV2: FUNCTION;
AXIOM
[1] APP(NIL,L)==L;
[2] APP(CONS(A,L),M)==CONS(A,APP(L,M));
[3] REV(NIL)==NIL;
[4] REV(CONS(A,L))==APP(REV(L),CONS(A,NIL));
[5] REV2(NIL,L)==L;
[6] REV2(CONS(A,L),M)==REV2(L,CONS(A,M));
END
