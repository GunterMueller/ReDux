DATATYPE LIST;
SORT
	EL, LIST;
EXTERNAL
	SAC2LIST: XREAD=LREAD, XWRITE=LWRITE;
	SAC2OBJ: XREAD=UREAD, XWRITE=UWRIT1;
CONST
	0: EL;
	NIL: LIST;
VAR
	A, B, C: EL;
	L, M, N: LIST;
OPERATOR
	APP: LIST, LIST -> LIST;
	CONS: EL, LIST -> LIST;
	REV: LIST -> LIST;
COERCION
	": SAC2LIST -> LIST;
	': SAC2OBJ -> EL;
NOTATION
	APP, CONS, REV: FUNCTION;
	": ROUNDFIX ";
	': ROUNDFIX ';
PROPERTY
	REV: XINT=CINV;
	CONS: XINT=COMP;
	APP: XINT=CCONC;
	APP: COERC=";
AXIOM
  [1] APP(NIL,L)==L;
  [2] APP(CONS(A,L),M)==CONS(A,APP(L,M));
  [3] REV(NIL)==NIL;
  [4] REV(CONS(A,L))==APP(REV(L),CONS(A,NIL));
END
